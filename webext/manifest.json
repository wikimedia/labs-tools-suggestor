{
  "manifest_version": 2,
  "name": "Suggestor",
  "version": "0.0.0",
  "description": "Suggest edits.",
  "applications": {
    "gecko": {
      "id": "suggestor@wmflabs",
      "strict_min_version": "45.0"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/wiki/*"],
      "js": [ "index.js" ]
    }
  ]
}
